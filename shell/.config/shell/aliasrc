#!/usr/bin/env sh

alias a='ace'
alias da='deactivate'
alias arc="vim $HOME/.config/shell/aliasrc"
alias bl="brew list"
alias bs="brew services"
alias c="clear" 
alias celar="clear"
alias claer="clear"
alias d="docker"
alias db="docker build"
alias dc="docker-compose"
alias di="docker images"
alias dl="docker logs"
alias dps="docker ps"
alias ds="docker service"
alias dirm="docker image rm --force"
alias dk="docker ps | awk '{print $8}' | grep -v NAMES | xargs -I {} docker kill --force {}"
alias dr="docker run"
alias drd="docker images --filter dangling=true -q | xargs -I {} docker rmi {}"
alias dv="docker volume"
alias dwn="cd $HOME/Downloads/"
alias egrep="egrep --color=always"
alias frc="vim $HOME/.config/shell/funcrc"
alias ga="git add"
alias gap="git add -p"
alias gb="git branch"
alias gc="git commit -m"
alias gco="git checkout"
alias gd="git diff"
alias gds="git diff --staged"
alias gf="git fetch"
alias g="cd $HOME/github/ && ls"
alias ghh="/usr/local/bin/gh"
alias gl="git log"
alias glo="git log --oneline"
alias glp="git log -p"
alias gp="git push"
alias gr="git rebase"
alias grs="git restore --staged"
alias grv="git remote -v"
alias greset="git reset HEAD"
alias gs="git status"
alias gw="git worktree"
alias gwa="git worktree add"
alias grep="grep --color -i"
alias j="just"
alias k="kubectl"
alias la="ls -A"
alias ll="ls -lh --octal-permissions"
alias lsd="ls -F | sed -n '/\//p'| sort -f"
alias ls="exa"
alias lss="/bin/ls"
alias mkdir="mkdir -p"
alias p="python"
alias pi="pip install"
alias packer="nvim ~/.config/nvim/lua/aaraney/packer.lua"
alias pip="python -m pip"
alias rp="source $HOME/.zshrc; source $HOME/.profile"
alias rmrf="rm -rf"
alias skrc="vim $HOME/.config/skhd/skhdrc"
alias sl="ls"
alias t="pytest"
alias vim="nvim"
alias zrc="vim $HOME/.zshrc"
